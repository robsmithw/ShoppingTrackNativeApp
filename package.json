    "lint": "eslint *.tsx",
    "code-formatter": "prettier *.tsx --write",
    "code-formatter-check": "prettier --check *.tsx",
    "types-check": "tsc --project . --noEmit --pretty --skipLibCheck",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
